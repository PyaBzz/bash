alias g='clear && git status && echo -ne "\033]0;$(basename `git rev-parse --show-toplevel`)\a"'
alias giff='clear && git diff'
alias gadd='clear && git add -p'
alias getch='git fetch && g'
alias gull='clear && git pull'
alias gush='clear && git push'
alias geckout='clear && git checkout'
alias gistash='clear && git stash'
alias granch='clear && git branch'
alias gommit='clear && git commit -m'
alias gamend='clear && git commit --amend'
alias glog='clear && git log'
alias gipe='git diff HEAD --name-only | xargs '
alias gmod='gipe chmod -x && gipe ll'
